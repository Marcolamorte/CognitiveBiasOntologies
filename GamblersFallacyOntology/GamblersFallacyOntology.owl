    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Activity -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Activity">
        <rdfs:comment>This frame represent an activity with random or chance-based outcomes in which a person is involved in.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ColorNumber -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ColorNumber">
        <rdfs:subClassOf rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Outcome"/>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Event -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Event"/>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#FutureEvent -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#FutureEvent">
        <rdfs:subClassOf rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Event"/>
        <rdfs:comment>This class represent an event that will happen in the next future.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Outcome -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Outcome">
        <rdfs:comment>This class represent the outcome of an event.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Person -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Person">
        <rdfs:comment>Any individual involved in an activity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Probability -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Probability">
        <rdfs:comment>This frame characterizes the likelihood that a Hypothetical_event will happen as a position on a scale of impossible to inevitable.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#RandomOccurrence -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#RandomOccurrence">
        <rdfs:subClassOf rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#FutureEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#isPartOf"/>
                <owl:someValuesFrom rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Sequence -->

    <owl:Class rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Sequence">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#generatedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This frame represent a sequence of temporally-ordered past events that have the same outcome</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Black_Number -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Black_Number">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ColorNumber"/>
        <rdfs:comment>The outcome of the next number draw or past number draws. Black numbers and red numbers have the same probability of being extracted.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Draw -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Draw">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Activity"/>
        <rdfs:comment>an activity where numbers are of two different colours (black and red) and where only one number is extracted for each draw.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Participant -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Participant">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Person"/>
        <hasBelief rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Past_Number_Draw_Series"/>
        <hasBiasedOpinion rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#NumberDraw"/>
        <isParticipantIn rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Draw"/>
        <rdfs:comment>An human being that participates to a lottery draw.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#NumberDraw -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#NumberDraw">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#RandomOccurrence"/>
        <hasParameter rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ProbabilityLevel"/>
        <isPartOf rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Draw"/>
        <produces rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Red_Number"/>
        <rdfs:comment>a number draw in a lottery is an event totally random where each number has the same probability of being extracted.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Past_Number_Draw_Series -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Past_Number_Draw_Series">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Sequence"/>
        <generatedBy rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Lottery_Draw"/>
        <hasOutcome rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Black_Number"/>
        <rdfs:comment>An ordered sequence of past number draws that has the same outcome (black) will influence the judgment of the participant about the probability of the outcome in the next number draw.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ProbabilityLevel -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ProbabilityLevel">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Probability"/>
        <influencedBy rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Past_Number_Draw_Series"/>
        <hasParameterDataValue>High</hasParameterDataValue>
        <rdfs:comment>The level of probability that the outcome of the next number draw will be red or black  can be higher or lower for the participant, depending on the outcomes of the previous number draws.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Red_Number -->

    <owl:NamedIndividual rdf:about="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#Red_Number">
        <rdf:type rdf:resource="http://www.corradoconsiglio.it/CognitiveBiasOntologies/GamblersFallacyOntology.owl#ColorNumber"/>
        <rdfs:comment>The outcome of the next number draw or past number draws. Black numbers and red numbers have the same probability of being extracted</rdfs:comment>
    </owl:NamedIndividual>
    


